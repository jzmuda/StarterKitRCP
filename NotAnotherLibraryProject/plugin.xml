<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="not.another.library.project.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="Perspective"
            class="not.another.library.perspectives.Perspective"
            id="NotAnotherLibraryProject.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="not.another.library.views.StartView"
            id="NotAnotherLibraryProject.startview"
            name="startview"
            restorable="true">
      </view>
      <view
            class="not.another.library.views.BookViev"
            id="NotAnotherLibraryProject.booksview"
            name="booksview"
            restorable="true">
      </view>
      <view
            class="not.another.library.views.AddView"
            id="NotAnotherLibraryProject.addview"
            name="addview"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="NotAnotherLibraryProject.perspective">
         <view
               closeable="false"
               id="NotAnotherLibraryProject.startview"
               minimized="false"
               relationship="left"
               relative="org.eclipse.ui.editorss"
               standalone="true"
               visible="true">
         </view>
         <view
               closeable="false"
               id="NotAnotherLibraryProject.booksview"
               minimized="false"
               relationship="right"
               relative="NotAnotherLibraryProject.startview"
               visible="false">
         </view>
         <view
               closeable="true"
               id="NotAnotherLibraryProject.addview"
               minimized="false"
               relationship="right"
               relative="NotAnotherLibraryProject.booksview"
               visible="false">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               label="File">
            <command
                  commandId="org.eclipse.ui.file.exit"
                  label="Exit">
            </command>
         </menu>
      </menuContribution>
   </extension>

</plugin>
